# Flask Configuration
FLASK_ENV=development
SESSION_SECRET=your-secret-key-here-change-in-production

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/aimarket
# For SQLite (development): DATABASE_URL=sqlite:///marketplace.db

# LLM API Keys
ANTHROPIC_API_KEY=your-anthropic-key
OPENAI_API_KEY=your-openai-key

# LangSmith Tracing (Observability for LangGraph agents)
# Sign up at https://smith.langchain.com to get your API key
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_PROJECT=ai-market-v2-local

# Google OAuth
AI_PIJACA_GOOGLE_CLIENT_ID=your-google-client-id
AI_PIJACA_GOOGLE_CLIENT_SECRET=your-google-client-secret

# SendGrid (Email)
SENDGRID_API_KEY=your-sendgrid-key
SENDGRID_FROM_EMAIL=noreply@aipijaca.com

# Replit (if using Replit Auth)
REPLIT_DOMAINS=your-replit-domains

# Other Services (if needed)
PINECONE_API_KEY=your-pinecone-key
